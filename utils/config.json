{
  "baseDirStructure": [
    "imports/startup/client",
    "imports/startup/server",
    "imports/api",
    "imports/ui/components",
    "imports/ui/layouts",
    "imports/ui/pages",
    "imports/ui/stylesheets",
    "client",
    "server",
    "public",
    "private"
  ],
  "gitBoilerplate": "brenopanzolini/meteor-boilerplate#master",
  "generate" : {
    "api": [
      { "source": "methods.js", "dest": "imports/api/__name/methods.js" },
      { "source": "model.js", "dest": "imports/api/__name/__modelName.js" },
      { "source": "publications.js", "dest": "imports/api/__name/server/publications.js" }
    ]
  }
}
